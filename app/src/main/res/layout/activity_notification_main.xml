<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Notifications.NotificationMain"
    android:orientation="vertical"
    android:padding="16dp">

    <Button
        android:id="@+id/button_schedule_notification"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Schedule Notification"
        android:onClick="scheduleChannel"/>
    <Button
        android:id="@+id/button_cancel_notification"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cancel Notification"
        android:onClick="cancelNotification"/>




</LinearLayout>